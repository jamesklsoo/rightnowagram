<h1>New User</h1>
<div class="row">
  <div class="col-md-1">
  </div>
  <div class="col-md-3">
    <h2>Sign Up</h2>
    <br>
    <div id="user">
      <%= form_for @user do |form| %>
        <%= render partial: '/users/form', object: form %>
        <%= form.submit %>
      <% end %>
    </div>
  </div>
  <div class="col-md-1">
    <%= link_to "Sign In", new_session_path, class: 'btn btn-primary' %>
  </div>
</div>
<%= link_to 'Back', users_path %>